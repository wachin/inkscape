msgid "Save Guides"
msgstr "안내선 저장"

#: ../share/extensions/gimp_xcf.inx:10
msgid "Save Grid"
msgstr "격자 저장"

#: ../share/extensions/gimp_xcf.inx:11
msgid "Save Background"
