#: ../src/ui/dialog/inkscape-preferences.cpp:2813 ../share/ui/units.xml:4
#: ../share/ui/units.xml:5
msgid "%"
msgstr "%"

#: ../src/ui/dialog/inkscape-preferences.cpp:2667
